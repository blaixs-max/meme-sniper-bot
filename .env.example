# BNB Chain Configuration
# Primary RPC endpoint - will fallback to others if this fails
RPC_URL=https://bsc-dataseed1.binance.org
WSS_URL=wss://bsc-ws-node.nariox.org:443

# Wallet Configuration
# IMPORTANT: Never commit your actual private key!
# Encrypted with AES-256 - use the CLI to set this up
PRIVATE_KEY_ENCRYPTED=
ENCRYPTION_KEY=

# Twitter API v2 Credentials (Optional - for social analysis)
# Get these from https://developer.twitter.com/
TWITTER_BEARER_TOKEN=
TWITTER_API_KEY=
TWITTER_API_SECRET=

# Trading Configuration
# Maximum BNB to spend per trade
MAX_BUY_AMOUNT=0.1
# Default slippage tolerance (percentage)
DEFAULT_SLIPPAGE=5
# Gas price multiplier for faster transactions
GAS_PRICE_MULTIPLIER=1.2

# Risk Management
# Stop loss percentage (e.g., 20 = -20%)
STOP_LOSS_PERCENT=20
# Take profit levels (comma-separated percentages)
TAKE_PROFIT_LEVELS=50,100,200
# Maximum concurrent positions
MAX_POSITIONS=5
# Daily trading limit in BNB
DAILY_LIMIT_BNB=1

# Security Settings
# Enable honeypot check before buying
HONEYPOT_CHECK_ENABLED=true
# Enable rug pull detection
RUG_CHECK_ENABLED=true
# Minimum liquidity in BNB to consider a token
MIN_LIQUIDITY_BNB=1

# Twitter Analysis (Optional)
# Minimum Twitter score to consider buying (0-100)
MIN_TWITTER_SCORE=30
# Enable Twitter sentiment analysis
TWITTER_ANALYSIS_ENABLED=false

# Logging
# Log level: debug, info, warn, error
LOG_LEVEL=info
# Log to file
LOG_TO_FILE=true
